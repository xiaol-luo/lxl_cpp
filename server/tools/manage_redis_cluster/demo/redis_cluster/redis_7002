cluster-enabled yes
cluster-node-timeout 5000
appendonly yes

daemonize yes
tcp-backlog 128
bind 0.0.0.0
dir /shared/zone/zone_0/redis_cluster/run

# different between each redis node
port 7002
pidfile redis_7002.pid
logfile redis_7002.log
